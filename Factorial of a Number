
#include <stdio.h>

int main() {
    int n, i;
    unsigned long long factorial = 1;

    printf("Enter an integer: ");
    scanf("%d", &n);

    if(n < 0)
        printf("Factorial does not exist for negative numbers.\n");
    else {
        for(i = 1; i <= n; ++i)
            factorial *= i;
        printf("Factorial of %d = %llu\n", n, factorial);
    }

    return 0;
}

output
Enter an integer: 5
Factorial of 5 = 120
